        "@types/helmet": "0.0.45",
        "@types/jsonwebtoken": "^8.3.8",
        "@types/mocha": "^7.0.2",
        "@types/mongodb": "^3.5.2",
        "@types/serve-favicon": "^2.5.0",
        "axios": "^0.19.2",
        "mocha": "^7.1.1",
        "nodemon": "^2.0.2",
        "shx": "^0.3.2",
        "ts-mocha": "^7.0.0",
        "ts-node-dev": "^1.0.0-pre.44",
        "tslint": "^6.1.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.8.3"
    }
