{
    "name": "@vrhood/backend",
    "description": "API for vr*Hood Web App",
    "version": "0.0.0",
    "homepage": "vrhood.de",
    "private": true,
    "main": "src",
    "keywords": [
        "wirvsvirus",
        "vrhood",
        "feathers",
        "typescript",
        "node"
    ],
    "author": {
        "name": "vr*Hood",
        "email": "info@vrhood.de"
    },
    "contributors": [
        {
            "name": "Sebastian Homeier",
            "email": "s.homeier@bynary.io"
        },
        {
            "name": "Johannes Homeier",
            "email": "j.homeier@bynary.io"
        },
        {
            "name": "Michael Stahl"
        },
        {
            "name": "Jakob Niggel"
        }
    ],
    "bugs": {},
    "directories": {
        "lib": "src",
        "test": "test/",
        "config": "config/"
    },
    "engines": {
        "node": "^12.0.0",
        "npm": ">= 3.0.0"
    },
    "scripts": {
        "test": "npm run compile && npm run mocha",
        "dev": "ts-node-dev --no-notify src/",
        "start": "npm run compile && node lib/",
        "mocha": "ts-mocha \"test/**/*.ts\" --recursive --exit",
        "compile": "shx rm -rf lib/ && tsc",
        "lint": "tslint -p .",
        "lintfix": "tslint -p . --fix"
    },
    "standard": {
        "env": [
            "mocha"
        ],
        "ignore": []
    },
    "types": "lib/",
    "dependencies": {
        "@feathersjs/authentication": "^4.5.2",
        "@feathersjs/authentication-local": "^4.5.2",
        "@feathersjs/authentication-oauth": "^4.5.2",
        "@feathersjs/configuration": "^4.5.2",
        "@feathersjs/errors": "^4.5.2",
        "@feathersjs/express": "^4.5.2",
        "@feathersjs/feathers": "^4.5.2",
        "@feathersjs/socketio": "^4.5.2",
        "@feathersjs/transport-commons": "^4.5.2",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "feathers-mongodb": "^6.1.0",
        "helmet": "^3.21.3",
        "mongodb": "^3.5.5",
        "mongodb-core": "^3.2.7",
        "serve-favicon": "^2.5.0",
